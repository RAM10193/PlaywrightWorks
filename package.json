{
  "name": "playwrightworks",
  "version": "1.0.0",
  "description": "Selenium examples using TypeScript and BrowserStack",
  "main": "index.js",
  "scripts": {
    "test": "percy exec -- playwright test",
    "testnormal": "npx playwright test",
    "testchrome": "npx playwright test --project=chromium",
    "testBS": "npx playwright test pomtests/registerandLogin.test.ts",
    "report": "npx playwright show-report",
    "postinstall": "npm update browserstack-node-sdk"
  },
  "keywords": [
    "browserstack",
    "selenium",
    "automate",
    "examples",
    "TypeScript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.35.1"
  },
  "dependencies": {
    "@percy/cli": "^1.26.1",
    "@percy/playwright": "^1.0.4",
    "browserstack-local": "^1.5.3",
    "browserstack-node-sdk": "^1.22.0"
  }
}
