{
  "name": "playwrightworks",
  "version": "1.0.0",
  "description": "Selenium examples using TypeScript and BrowserStack",
  "main": "index.js",
  "scripts": {
    "testpercy": "percy exec -- playwright test",
    "test": "npx playwright test",
    "testchrome": "npx playwright test --project=chromium",
    "testBS": "npx playwright test pomtests/registerandLogin.test.ts",
    "report": "npx playwright show-report",
    "postinstall": "npm update browserstack-node-sdk",
    "test:bdd": "cucumber-js --require cucumber.config.ts --require tests/bddtests/stepdefinitions/**/*.ts --format @cucumber/pretty-formatter"
  },
  "keywords": [
    "browserstack",
    "selenium",
    "automate",
    "examples",
    "TypeScript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.35.1"
  },
  "dependencies": {
    "@cucumber/cucumber": "^9.3.0",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@percy/cli": "^1.26.1",
    "@percy/playwright": "^1.0.4",
    "browserstack-local": "^1.5.3",
    "browserstack-node-sdk": "^1.22.0"
  }
}
